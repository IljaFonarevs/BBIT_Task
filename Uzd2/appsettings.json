{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=master;Trusted_Connection=True;TrustServerCertificate=True;Encrypt=False"
  },
  "AllowedHosts": "*",
  "CorsPolicy": {
    "Origins": [ "http://localhost" ],
    "Headers": [ "*" ],
    "Methods": [ "GET", "POST", "PUT", "DELETE" ]
  },

  "Jwt": {
    "Issuer": "yourdomain.com",
    "Audience": "yourdomain.com",
    "Key": "ThisIsA32CharacterLongSecretKey!"
  }
}
